{

  // settings for high log volume
  max_line_size: 49900,
  max_allowed_request_size: 5900000,
  pipeline_threshold: 0,
  min_request_spacing_interval: 0.1,
  max_request_spacing_interval: 3.0,
  max_log_offset_size: 400000000,
  max_existing_log_offset_size: 400000000,

  // Comment/uncomment the following line to disable/enable compression of request data.
  // Compressed data will use less outgoing bandwidth, but will increase CPU load of
  // individual agents
  compression_type: "deflate",
  compression_level: 3, // Setting to a higher value will improve compression, at the expense of CPU

  // Enable profiling - this will enable us to help troubleshoot performance problems
  // ONLY USE FOR LOAD TESTS
  enable_profiling: true,
  profile_duration_minutes: 2,
  max_profile_interval_minutes: 60,

  // Fields describing this server. These fields are attached to each log message, and
  // can be used to filter data from a particular server or group of servers.
  server_attributes: {
     // Fill in this field if you'd like to override the server's hostname.
     // serverHost: "REPLACE THIS",
  }
}

